Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"MatDirSum", "[", "sqMatrices_", "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"t", ",", "dims", ",", "rowPaddings"}], "}"}], ",", 
      RowBox[{
       RowBox[{"dims", "=", 
        RowBox[{"Prepend", "[", 
         RowBox[{
          RowBox[{"Length", "/@", "sqMatrices"}], ",", "0"}], "]"}]}], ";", 
       RowBox[{"t", "=", 
        RowBox[{"Total", "[", "dims", "]"}]}], ";", "\n", 
       RowBox[{"rowPaddings", "=", 
        RowBox[{"Rest", "[", 
         RowBox[{"FoldList", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"#1", "+", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"#2", "[", 
                RowBox[{"[", "1", "]"}], "]"}], ",", 
               RowBox[{"-", 
                RowBox[{"#2", "[", 
                 RowBox[{"[", "2", "]"}], "]"}]}]}], "}"}]}], "&"}], ",", 
           RowBox[{"{", 
            RowBox[{"0", ",", "t"}], "}"}], ",", 
           RowBox[{"Partition", "[", 
            RowBox[{"dims", ",", "2", ",", "1"}], "]"}]}], "]"}], "]"}]}], 
       ";", "\n", 
       RowBox[{"Join", "@@", 
        RowBox[{"MapThread", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"ArrayPad", "[", 
            RowBox[{"#1", ",", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"{", "0", "}"}], ",", "#2"}], "}"}]}], "]"}], "&"}], 
          ",", 
          RowBox[{"{", 
